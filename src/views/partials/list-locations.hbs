<table class="table is-fullwidth">
  <thead>
    <tr>
      <th>Location Name</th>
      <th>Latitiude</th>
      <th>Longitude</th>
      <th>Category</th>
      <th>Size</th>
    </tr>
  </thead>
  <tbody>
    {{#each geocache.locations}}
      <tr>
        <td>
          {{name}}
        </td>
        <td>
          {{latitude}}
        </td>
        <td>
          {{longitude}}
        </td>
        <td>
          {{category}}
        </td>
        <td>
          {{size}}
        </td>
          <td>
          <a href="/geocache/{{../geocache._id}}/location/{{_id}}" class="ui icon button">
            <i class="fas fa-pen"></i>
            </a>
        </td>
         <td>
          <a href="/geocache/{{../geocache._id}}/deleteLocation/{{_id}}" class="ui icon button">
            <i class="fas fa-trash"></i>
          </a>
        </td>
      </tr>
    {{/each}}
  </tbody>
</table>
